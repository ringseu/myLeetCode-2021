class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        vector<int> result;
        unordered_set<int> tmp;

        for (int i = 0; i < nums.size(); ++i)
        {
            if (tmp.end() != tmp.find(nums[i])) //之前出现过
            {
                result.push_back(nums[i]);
            }
            else
            {
                tmp.insert(nums[i]);
            }
        }

        return result;
    }
};
