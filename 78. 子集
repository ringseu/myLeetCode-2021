class Solution {
public:
    vector<vector<int>> subsets(vector<int>& nums) {
        vector<vector<int>> result;
        vector<int> tmp;

        backtrace(nums, 0, tmp, result);

        return result;
    }

    void backtrace(vector<int> &nums, int start, vector<int> &tmp, vector<vector<int>> &result)
    {
        result.push_back(tmp);
        if (start >= nums.size())
        {
            return;
        }

        for (int i = start; i < nums.size(); ++i)
        {
            tmp.push_back(nums[i]);
            backtrace(nums, i+1, tmp, result);
            tmp.pop_back();
        }
    }
};
